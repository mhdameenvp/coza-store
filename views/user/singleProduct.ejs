		<script>
			document.addEventListener('DOMContentLoaded', function () {
				const btnDecrease = document.querySelector('.btn-num-product-down');
				const btnIncrease = document.querySelector('.btn-num-product-up');
				const inputQuantity = document.querySelector('#quantity');
				const product = document.getElementById('productName');
				const productName = product.getAttribute('data-value');
				const button = document.getElementById('addtocart');

				btnDecrease.addEventListener('click', function () {
					let currentValue = parseInt(inputQuantity.value, 10);
					if (currentValue > 1) {
						inputQuantity.value = currentValue - 1;
					}
				});

				btnIncrease.addEventListener('click', function () {
					let currentValue = parseInt(inputQuantity.value, 10);
					inputQuantity.value = currentValue + 1;
				});

				const id = button.getAttribute('data-name');
				console.log('id in single product page:', id);
				axios.get(`/addcart/${id}`)
					.then(res => {
						if (res.data.exist == true) {
							button.innerHTML = 'Go to cart'

						} else {
							button.innerHTML = 'Add to  cart'
						}


					}).catch((err) => {
						console.log('error in finding cart exist ');
					})

				button.addEventListener('click', function (e) {
					e.preventDefault();



					if (button.innerHTML === 'Go to cart') {
						window.location.href = `/shoppingCart`

					} else {




						axios.post(`/addcart`, { productId: id })
							.then(res => {
								const cartCount = res.data.cartCount
								var cartElement = document.getElementById("cartIcon");
								cartElement.setAttribute('data-notify', cartCount);
								Swal.fire({
									title: 'Success!',
									text: `${productName} Item is added to add to cart!`,
									icon: 'success',
									confirmButtonText: 'OK'
								});

								button.innerHTML = 'Go to cart'

							})
							.catch(error => {
								console.error("Error adding to cart:", error);
								Swal.fire({
									title: 'Error!',
									text: 'There was a problem adding the item to the cart.',
									icon: 'error',
									confirmButtonText: 'OK'
								});
							});
					}
				});
			});

		</script>

